public class IncrementDecrement 
{

	public static void main(String[] args) 
	{
		
		// Incrementar
		
		for (int i = 1; i <= 10 ; i++)
		{
			System.out.println(i + " ");
		}
		
		
		System.out.println();
		
		// Decrementar
		
		for (int i = 10; i >= 1 ; i--)
		{
			System.out.print(i + " ");
		}
		
	}

}
