package principal;
import entidadeDAO.AlunoDAO;
import entidades.Aluno;

public class MenuPrincipal 
{

	public static void main(String[] args) 
	{
		Aluno aluno = new Aluno();
		aluno.setNomeAluno("Joana");
		aluno.setNomeMae("Joaquina");
		aluno.setNomePai("Joaquim");
		aluno.setDataNasc(Aluno.formataDataNascAluno("20/10/1990"));
		aluno.setMediaAluno(7.54);
		aluno.setSituacaoAluno("M");
		
		if (new AlunoDAO().salvar(aluno))
		{
			System.out.println("Sucesso.");
		}
		
		else 
		{
			System.out.println("Falhou.");
		}
		
	}

}
